{
  "name": "@rondinif/ita-webpack5-labs",
  "version": "0.1.0-alpha.0",
  "description": "Learning labs with accompanying notes in Italian to dive into webpack 5 eco-system",
  "private": true,
  "author": "rondinif",
  "license": "MIT",
  "scripts": {
    "build": "npm --ignore-scripts=false run build-lab-01 && npm --ignore-scripts=false run build-lab-02a && npm --ignore-scripts=false run build-lab-02b && npm --ignore-scripts=false run build-lab-02c && npm --ignore-scripts=false run build-lab-03 && npm --ignore-scripts=false run build-lab-04 && npm --ignore-scripts=false run build-lab-05 && npm --ignore-scripts=false run build-lab-06 && npm --ignore-scripts=false run build-lab-07 && npm --ignore-scripts=false run build-lab-08 && npm --ignore-scripts=false run build-lab-09 && npm --ignore-scripts=false run  build-lab-83-pug && npm --ignore-scripts=false run build-lab-83-pug-adv-a && npm --ignore-scripts=false run build-lab-pug-01 && npm --ignore-scripts=false run build-lab-pug-01-adv-a && npm --ignore-scripts=false run build-lab-pug-02 && npm --ignore-scripts=false run build-lab-pug-02-adv-a",
    "build-docs": "npx webpack --config src/docs/webpack.config.js",
    "debug-docs": "node --inspect-brk=5858 node_modules/webpack/bin/webpack.js --colors --config src/docs/webpack.config.js --mode=development",
    "ex01": "rm -rf dist/ex01; webpack-cli --config=./src/ex01/webpack.config-ex01.js --mode=development --output=dist/ex01/ex01.js --entry=./src/ex01/src/file.js",
    "build-lab-01": "rm -rf dist/lab-01; npx webpack-cli --config=./src/lab-01/webpack.config.js --mode=development",
    "build-lab-02a": "rm -rf dist/lab-02 & npx webpack --config src/lab-02/url-loader.webpack.config.js --mode=development",
    "build-lab-02b": "rm -rf dist/lab-02 & npx webpack --config src/lab-02/file-loader-1.webpack.config.js --mode=development",
    "build-lab-02c": "rm -rf dist/lab-02 & npx webpack --config src/lab-02/file-loader-2.webpack.config.js --mode=development",
    "build-lab-03": "rm -rf dist/lab-03 & npx webpack --config src/lab-03/webpack.config.js --mode=development",
    "build-lab-04": "rm -rf dist/lab-04 & npx webpack --config src/lab-04/webpack.config.js --mode=development",
    "build-lab-05": "rm -rf dist/lab-05 & npx webpack --config src/lab-05/webpack.config.js --mode=development",
    "build-lab-06": "rm -rf dist/lab-06 & npx webpack --config src/lab-06/webpack.config.js --mode=development",
    "build-lab-07": "rm -rf dist/lab-07 & npx webpack --config src/lab-07/webpack.config.js --mode=development",
    "build-lab-08": "npx webpack --config src/lab-08/webpack.config.js --mode=development",
    "build-lab-09": "npx webpack --config src/lab-09/webpack.config.js --mode=development",
    "watch-lab-09": "npx webpack --config src/lab-09/webpack.config.js --mode=development --watch",
    "start-lab-10": "npx webpack-dev-server --open --config src/lab-10/webpack.config.js --mode=development",
    "start-lab-11": "node ./src/lab-11/src/server.js",
    "build-lab-83": "npx webpack --config src/lab-83/webpack.config.js --mode=development",
    "build-lab-83-pug": "npx webpack --config src/lab-83-pug/webpack.config.js --mode=development",
    "build-lab-83-pug-adv-a": "npx webpack --config src/lab-83-pug/webpack.config-advanced-a.js",
    "start-lab-83-pug-adv-a": "webpack-dev-server --config src/lab-83-pug/webpack.config-advanced-a.js --mode=development",
    "build-lab-pug-01": "npx webpack --config src/lab-pug-01/webpack.config.js --mode=development",
    "build-lab-pug-01-adv-a": "npx webpack --config src/lab-pug-01/webpack.config-advanced-a.js",
    "start-lab-pug-01-adv-a": "webpack-dev-server --config src/lab-pug-01/webpack.config-advanced-a.js --mode=development",
    "build-lab-pug-02": "npx webpack --config src/lab-pug-02/webpack.config.js --mode=development",
    "build-lab-pug-02-adv-a": "npx webpack --config src/lab-pug-02/webpack.config-advanced-a.js",
    "start-lab-pug-02-adv-a": "webpack-dev-server --config src/lab-pug-02/webpack.config-advanced-a.js --mode=development",
    "build-lab-pug-03": "npx webpack --config src/lab-pug-03/webpack.config.js --mode=development",
    "buildprod-lab-pug-03": "npx webpack --config src/lab-pug-03/webpack.config.js --mode=production",
    "buildx-lab-pug-03": "node --inspect node_modules/.bin/webpack --config src/lab-pug-03/webpack.config.js --mode=development",
    "debug-lab-pug-03": "node --inspect-brk=5858 node_modules/webpack/bin/webpack.js --colors --config src/lab-pug-03/webpack.config.js --mode=development",
    "build-lab-pug-04": "npx webpack --config src/lab-pug-04/webpack.config.js --mode=development",
    "debug-lab-pug-04": "node --inspect-brk=5858 node_modules/webpack/bin/webpack.js --colors --config src/lab-pug-04/webpack.config.js --mode=development",
    "test": "echo \"Attenzione: per questi laboratori non sono previsti tests\" && exit 0"
  },
  "devDependencies": {
    "acorn": "^8.5.0",
    "apply-loader": "^2.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "commitlint": "^13.1.0",
    "css-loader": "^6.2.0",
    "csv-loader": "^3.0.3",
    "dotenv-flow-webpack": "^1.1.0",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "findup-sync": "^4.0.0",
    "html-webpack-plugin": "^5.3.2",
    "kind-of": "^6.0.3",
    "minimist": ">=1.2.5",
    "pug-loader": "^2.4.0",
    "style-loader": "^3.2.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-middleware": "^5.1.0",
    "webpack-dev-server": "^4.2.1",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "pug": "^3.0.2"
  }
}
